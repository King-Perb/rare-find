{
  "name": "@rare-find/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type:check": "tsc --noEmit",
    "prepare": "npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema=./prisma/schema.prisma",
    "db:push": "npx prisma db push --schema=./prisma/schema.prisma",
    "db:pull": "npx prisma db pull --schema=./prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema=./prisma/schema.prisma",
    "db:setup:dev": "npx prisma migrate dev --name init --schema=./prisma/schema.prisma",
    "db:reset:dev": "npx prisma migrate reset --schema=./prisma/schema.prisma",
    "db:deploy": "npx prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:test-connection": "tsx scripts/test-prisma-connection.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:coverage": "tsx scripts/check-e2e-coverage.ts"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-react": "^0.17.10",
    "@octokit/rest": "^22.0.1",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@rare-find/shared": "*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.556.0",
    "next": "^16.0.10",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "@supabase/supabase-js": "^2.87.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/pg": "^8.11.10",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "jsdom": "^25.0.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.16",
    "prisma": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@prisma/adapter-pg": "^7.0.0",
    "pg": "^8.13.1",
    "zod": "^3.25.67",
    "zod-prisma-types": "^3.2.4"
  }
}

