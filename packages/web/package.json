{
  "name": "@rare-find/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type:check": "tsc --noEmit",
    "prepare": "npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema=./prisma/schema.prisma",
    "db:push": "npx prisma db push --schema=./prisma/schema.prisma",
    "db:pull": "npx prisma db pull --schema=./prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema=./prisma/schema.prisma",
    "db:setup:dev": "npx prisma migrate dev --name init --schema=./prisma/schema.prisma",
    "db:reset:dev": "npx prisma migrate reset --schema=./prisma/schema.prisma",
    "db:deploy": "npx prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:generate-types": "tsx scripts/generate-supabase-types.js",
    "db:test-connection": "tsx scripts/test-prisma-connection.ts",
    "test": "vitest run --config vitest.config.unit.ts",
    "test:watch": "vitest --config vitest.config.unit.ts",
    "test:ui": "vitest --ui --config vitest.config.unit.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.unit.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:coverage": "tsx scripts/check-e2e-coverage.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-react": "^0.17.10",
    "@octokit/rest": "^22.0.1",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@rare-find/shared": "*",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/supabase-js": "^2.87.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.556.0",
    "next": "^16.0.10",
    "next-themes": "^0.4.6",
    "openai": "^6.15.0",
    "pg": "^8.13.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-mcp": "^0.1.6",
    "@storybook/addon-onboarding": "^10.1.10",
    "@storybook/addon-vitest": "^10.1.10",
    "@storybook/nextjs-vite": "^10.1.10",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "eslint-plugin-storybook": "^10.1.10",
    "jsdom": "^25.0.1",
    "playwright": "^1.57.0",
    "prisma": "^7.0.0",
    "prisma-zod-generator": "^2.1.2",
    "storybook": "^10.1.10",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
