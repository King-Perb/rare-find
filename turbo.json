{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {
            "outputs": []
        },
        "type:check": {
            "outputs": []
        },
        "test": {
            "outputs": []
        },
        "test:watch": {
            "cache": false,
            "persistent": true
        },
        "test:e2e": {
            "cache": false
        },
        "mobile:start": {
            "cache": false,
            "persistent": true
        },
        "mobile:ios": {
            "cache": false,
            "persistent": true
        },
        "mobile:android": {
            "cache": false,
            "persistent": true
        }
    }
}
